プロフィール編集
  <div class = "container">
    <%= form_with model:@user, url: admin_user_path(@user.id), method: :patch do |f| %>
   <div class ="row">
            <%= f.label :"名前（姓)" %>
            <%= f.text_field :last_name, autofocus: true, autocomplete: "last_name" %>
            （名）
            <%= f.text_field :first_name, autofocus: true, autocomplete: "first_name" %>
        </div>
        
        <div class="field">
            <%= f.label :"フリガナ" %>
            (セイ)
            <%= f.text_field :last_name_kana, autofocus: true, autocomplete: "last_name_kana" %>
            (メイ)
            <%= f.text_field :first_name_kana, autofocus: true, autocomplete: "first_name_kana" %>
        </div>
        
        <div class="field">
            <%= f.label :"メールアドレス" %>
            <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
        </div>
        
        <div class="field">
            <%= f.label :"電話番号（ハイフンなし）" %>
            <%= f.text_field :telephone_number, autofocus: true, autocomplete: "telephone_number" %>
        </div>
        
        <div class="field">
            <%= f.label :"ユーザーステータス" %>
            <%= f.label :is_active, "有効", value:true %>
            <%= f.radio_button :is_active,true %>
            <%= f.label :is_active, "無効", value:false %>
            <%= f.radio_button :is_active,false %>
        </div>
        
            <%= f.submit"変更を保存" %>
            <% end %>
  </div>  
    
 